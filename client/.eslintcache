ssword}\r\n            onChange={onChange}\r\n            required\r\n          />\r\n        </div>\r\n        <input\r\n          type=\"submit\"\r\n          value=\"Login\"\r\n          className=\"btn btn-primary btn-block\"\r\n        />\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\context\\alert\\AlertState.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\context\\alert\\alertReducer.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\context\\alert\\alertContext.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\components\\layout\\Alerts.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\utils\\setAuthToken.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\components\\routing\\PrivateRoute.js",[],"C:\\Users\\Navneet\\Documents\\Projects\\contact-keeper\\client\\src\\components\\layout\\Spinner.js",["112"],"import React from \"react\";\r\nimport spinner from \"./spinner.gif\";\r\n\r\nexport default () => (\r\n  <>\r\n    <img\r\n      src={spinner}\r\n      style={{ width: \"200px\", margin: \"auto\", display: \"block\" }}\r\n      alt=\"Loading...\"\r\n    />\r\n  </>\r\n);\r\n",{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":16,"column":3,"nodeType":"119","messageId":"120","endLine":16,"endColumn":16},{"ruleId":"117","severity":1,"message":"118","line":10,"column":3,"nodeType":"119","messageId":"120","endLine":10,"endColumn":16},{"ruleId":"117","severity":1,"message":"121","line":10,"column":11,"nodeType":"119","messageId":"120","endLine":10,"endColumn":19},{"ruleId":"122","severity":1,"message":"123","line":4,"column":1,"nodeType":"124","endLine":12,"endColumn":3},"no-native-reassign",["125"],"no-negated-in-lhs",["126"],"no-unused-vars","'REGI